{
  "name": "@dexwox-labs/a2a-server",
  "version": "0.1.0",
  "description": "TypeScript server implementation for Google's Agent-to-Agent (A2A) protocol - includes Express/WebSocket handlers, request validation and queue management",
  "homepage": "https://dexwox-innovations-org.github.io/a2a-node-sdk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dexwox-Innovations-Org/a2a-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Dexwox-Innovations-Org/a2a-node-sdk/issues"
  },
  "keywords": [
    "a2a",
    "agent-to-agent",
    "google",
    "protocol",
    "typescript",
    "server",
    "express",
    "websocket",
    "api",
    "rpc"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@dexwox-labs/a2a-core": "0.1.0",
    "@types/ws": "^8.18.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "winston": "^3.17.0",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.1.3",
    "supertest": "^7.1.1",
    "typescript": "^5.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "license": "Apache-2.0"
}
